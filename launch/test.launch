<?xml version="1.0"?>
<launch>
    <arg name="debug" default="0" />
    <param name="use_sim_time" value="true"/>
    
    <!-- RPG Flightmare Unity Render. -->
    <node pkg="flightrender" type="RPG_Flightmare.x86_64" name="rpg_flightmare_render" >
    </node>

    <node name="start_quadrotor_node" pkg="hdi_plan" type="start_quadrotor_node" output="screen" >
    </node>

    <node name="clock_server" pkg="hdi_plan" type="clock_server" output="screen" >
    </node>

</launch>